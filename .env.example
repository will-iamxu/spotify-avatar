# Spotify App Credentials
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
NEXT_PUBLIC_SPOTIFY_CLIENT_ID=your_spotify_client_id_here

# Replicate API
REPLICATE_API_TOKEN=your_replicate_api_token_here

# NextAuth
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Database Configuration
# Set USE_SECRETS_MANAGER=true to use AWS Secrets Manager for database credentials
USE_SECRETS_MANAGER=false
DATABASE_SECRET_NAME=your-database-secret-name

# Database URL (used when Secrets Manager is disabled or as fallback)
DATABASE_URL="postgresql://username:password@host:5432/database_name?schema=public&sslmode=require"

# AWS Configuration
AWS_REGION=us-east-2
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_S3_BUCKET_NAME=your_s3_bucket_name_here

# CloudWatch Logging (Optional)
CLOUDWATCH_LOG_GROUP=your_log_group_name_here

# Optional: Prisma Studio
PRISMA_STUDIO_PORT=5555